Algoritmo CalculoExpresion
    Definir r, a, b, c, d, resultado Como Real;

    // Solicitar al usuario que ingrese los valores de las variables
    Escribir "Ingrese el valor de r:"
    Leer r;

    Escribir "Ingrese el valor de a:"
    Leer a;

    Escribir "Ingrese el valor de b:"
    Leer b;

    Escribir "Ingrese el valor de c:"
    Leer c;

    Escribir "Ingrese el valor de d:"
    Leer d;

    // Calcular la expresi√≥n
    resultado <- (4.0  * (r + 34)) - 9 * (a + b * c) + ((3 + d * (2 + a)) / (a + b * d))/3

    // Imprimir el resultado
    Escribir "El resultado es:", resultado;


FinAlgoritmo
