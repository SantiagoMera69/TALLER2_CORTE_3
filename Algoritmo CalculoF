Algoritmo CalculoF
    Definir x Como Entero;
    Definir sigma Como Real;
    Definir y Como Entero;
    Definir lambda Como Real;
    Definir alfa Como Real;
    Definir f Como Real;

    x <- 12
    sigma <- 2.1836
    y <- 3
    lambda <- 1.11695
    alfa <- 328.67

    f <- 3 * (x + sigma * y / Potencia(x, 2) - Potencia(y, 2)) - lambda * (alfa - 13.7)

    Escribir "El valor de f es:",f;

FinAlgoritmo
